<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#32C8C8C8"
    android:orientation="vertical"
    tools:context=".Dashboard">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_190sdp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_150sdp"
        android:background="@drawable/round_box1"
        android:paddingLeft="@dimen/_20sdp"
        android:orientation="vertical"
        android:paddingTop="@dimen/_20sdp"
        android:backgroundTint="#18701C">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/_10sdp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
        <ImageView
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:background="@drawable/ic_baseline_arrow_back_24"
            android:backgroundTint="@color/white">
        </ImageView>
            </LinearLayout>

            <TextView
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="wrap_content"
                android:text="Dashboard"
                android:gravity="center"
                android:textColor="@color/white"
                android:elevation="@dimen/_15sdp"
                android:textStyle="bold"
                android:textSize="@dimen/_28sdp"
                ></TextView>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="end"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="@dimen/_25sdp"
                    android:layout_height="@dimen/_25sdp"
                    android:background="@drawable/ic_baseline_menu_24"
                    android:backgroundTint="@color/white">
                </ImageView>
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30sdp"
            android:background="@drawable/round_box2"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:backgroundTint="#DCFFFFFF">
            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:textSize="@dimen/_13sdp"
                android:textColor="#000"
                android:hint="Search details"
                android:paddingHorizontal="@dimen/_10sdp"
                android:textColorHint="#90393030"></EditText>
        </LinearLayout>


    </LinearLayout>
        <ImageView
            android:layout_width="@dimen/_80sdp"
            android:layout_gravity="bottom|end"
            android:layout_height="@dimen/_80sdp"
            android:src="@drawable/capsule"></ImageView>
    </FrameLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/_30sdp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginHorizontal="@dimen/_17sdp"
                android:elevation="@dimen/_15sdp"
                android:layout_height="wrap_content"
                android:id="@+id/medicinecost"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:scaleType="fitCenter"
                    android:elevation="@dimen/_15sdp"
                    app:riv_corner_radius="@dimen/_20sdp"
                    android:background="@drawable/round_box2"
                    android:backgroundTint="#A8719EB3"
                    android:padding="@dimen/_15sdp"
                    android:src="@drawable/medicineicon"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="Medicine Cost"></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginHorizontal="@dimen/_17sdp"
                android:elevation="@dimen/_15sdp"
                android:layout_height="wrap_content"
                android:id="@+id/bloodavailbility"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:scaleType="fitCenter"
                    app:riv_corner_radius="@dimen/_20sdp"
                    android:background="@drawable/round_box2"
                    android:elevation="@dimen/_15sdp"
                    android:backgroundTint="#ABF3A3BE"
                    android:padding="@dimen/_15sdp"
                    android:src="@drawable/syringe"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="Blood Availbility"></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginHorizontal="@dimen/_17sdp"
                android:elevation="@dimen/_15sdp"
                android:layout_height="wrap_content"
                android:id="@+id/pathologyprice"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:scaleType="fitCenter"
                    android:elevation="@dimen/_15sdp"
                    app:riv_corner_radius="@dimen/_20sdp"
                    android:background="@drawable/round_box2"
                    android:backgroundTint="#9E812C"
                    android:padding="@dimen/_15sdp"
                    android:src="@drawable/labicon"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="Pathology Price"></TextView>

            </LinearLayout>



        </LinearLayout>




    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginHorizontal="@dimen/_17sdp"
                android:elevation="@dimen/_15sdp"
                android:layout_height="wrap_content"
                android:id="@+id/raiseacomplain"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:scaleType="fitCenter"
                    app:riv_corner_radius="@dimen/_20sdp"
                    android:elevation="@dimen/_15sdp"
                    android:background="@drawable/round_box2"
                    android:backgroundTint="#FFAE94"
                    android:padding="@dimen/_15sdp"
                    android:src="@drawable/complain"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="Raise a complain"></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginHorizontal="@dimen/_17sdp"
                android:elevation="@dimen/_15sdp"
                android:layout_height="wrap_content"

                android:orientation="vertical">
                <ImageView
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:scaleType="fitCenter"
                    app:riv_corner_radius="@dimen/_20sdp"
                    android:background="@drawable/round_box2"
                    android:elevation="@dimen/_15sdp"
                    android:backgroundTint="#D3FFA1"
                    android:padding="@dimen/_15sdp"
                    android:src="@drawable/status"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="Claim Status"></TextView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_marginHorizontal="@dimen/_17sdp"
                android:elevation="@dimen/_15sdp"
                android:layout_height="wrap_content"

                android:orientation="vertical">
                <ImageView
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="@dimen/_70sdp"
                    android:scaleType="fitCenter"
                    app:riv_corner_radius="@dimen/_20sdp"
                    android:background="@drawable/round_box2"
                    android:elevation="@dimen/_15sdp"
                    android:backgroundTint="#829E9B"
                    android:padding="@dimen/_15sdp"
                    android:src="@drawable/history"></ImageView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:gravity="center"
                    android:text="History"></TextView>

            </LinearLayout>



        </LinearLayout>




    </LinearLayout>



</LinearLayout>